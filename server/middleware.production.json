{
  "files": {
    "loopback#static": [{
      "name": "default",
      "paths": ["/"],
      "params": "$!../client_dist"
    },
    {
      "name": "public",
      "paths": ["/public"],
      "params": "$!../client/public"
    }]
  },
  "final:after": {
    "loopback#errorHandler": {
      "params": {
        "includeStack": false
      }
    }
  }
}

